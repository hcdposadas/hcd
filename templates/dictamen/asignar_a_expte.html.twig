{% extends 'base.html.twig' %}

{% block title %}
    Asignar dictamen a Expediente
{% endblock title %}
{% block body %}
    <a class="btn btn-default" href="{{ path('dictamen_index') }}">Volver</a>
    <h1>Asignar dictamen a Expediente</h1>
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">Dictamen</h3>
        </div>
        <div class="card-body">

            {{ form_start(form) }}
            <h4>Expediente</h4>

            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.expediente) }}
                </div>
            </div>

            <h4>Dictamen</h4>

            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.fecha) }}
                </div>
            </div>

            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.tipoProyecto) }}
                </div>
            </div>
            {{ form_widget(form) }}
            <input class="btn btn-primary" type="submit" value="Crear"/>
            {{ form_end(form) }}
        </div>
    </div>


    <a class="btn btn-default" href="{{ path('dictamen_index') }}">Volver</a>
{% endblock body %}