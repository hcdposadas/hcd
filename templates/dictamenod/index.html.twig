{% extends 'base.html.twig' %}

{% block title %}
Dictamenods
{% endblock title %}
{% block body %}
<div class="card">
    <div class="card-header">
        <h3 class="card-title">Dictamenods</h3>
    </div>
    <div class="card-body">
        <table class="table table-bordered table-striped data-table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Extracto</th>
                <th>Tienetratamientopreferencial</th>
                <th>Activo</th>
                <th>Fechacreacion</th>
                <th>Fechaactualizacion</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for dictamenOD in dictamenODs %}
            <tr>
                <td><a href="{{ path('dictamenod_show', { 'id': dictamenOD.id }) }}">{{ dictamenOD.id }}</a></td>
                <td>{{ dictamenOD.extracto }}</td>
                <td>{% if dictamenOD.tieneTratamientoPreferencial %}Si{% else %}No{% endif %}</td>
                <td>{% if dictamenOD.activo %}Si{% else %}No{% endif %}</td>
                <td>{% if dictamenOD.fechaCreacion %}{{ dictamenOD.fechaCreacion|date('d/m/Y H:i:s') }}{% endif %}</td>
                <td>{% if dictamenOD.fechaActualizacion %}{{ dictamenOD.fechaActualizacion|date('d/m/Y H:i:s') }}{% endif %}</td>
                <td>
                        <a class="btn btn-default" href="{{ path('dictamenod_show', { 'id': dictamenOD.id }) }}">
                            <i class="fa fa-show"></i>
                                                    </a>
                        <a class="btn btn-default" href="{{ path('dictamenod_edit', { 'id': dictamenOD.id }) }}">
                            <i class="fa fa-edit"></i>
                                                    </a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
        </table>
    </div>
</div>

    <a class="btn btn-primary" href="{{ path('dictamenod_new') }}">Nuevo dictamenOD</a>
{% endblock %}
